{\n        setLoading(false);\n      }\n    };\n    getService();\n  }, [isAuthenticated, dispatch]);\n\n  useEffect(() => {\n    if (!isAuthenticated) {\n      return;\n    }\n    setLoading(true);\n    const fetchUsers = async () => {\n      try {\n        const response = await api(\"/users\");\n        dispatch(getUsers(response.data));\n      } catch (error) {\n        console.log(error.response);\n      } finally {\n        setLoading(false);\n      }\n    };\n    fetchUsers();\n  }, [isAuthenticated, dispatch]);\n\n  const routes = useRoutes();\n\n  if (loading) {\n    return <Loader />;\n  }\n\n  if (isFetching) {\n    return <Loader />;\n  }\n\n  return (\n    <Router>\n      {isAuthenticated && <Navbar />} <div>{routes}</div>\n    </Router>\n  );\n};\n","/Users/petrsavrasov/Projects/queueApp/client/src/store/roleReducer.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/store/userReducer.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/store/serviceReducer.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/routes.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/store/asyncActions.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/components/Loader.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/components/navbar.js",["369"],"import React, { useContext } from \"react\";\nimport { AdminMenu } from \"./Menu/AdminMenu\";\nimport { EmployeeMenu } from \"./Menu/EmployeeMenu\";\nimport { UserMenu } from \"./Menu/UserMenu\";\nimport { useSelector } from \"react-redux\";\n\nexport const Navbar = () => {\n  const { role } = useSelector((state) => state.userRole);\n  /* \n  const isSuperAdmin = role === \"superAdmin\" ? true : false;\n  const isAdmin = role === \"admin\" ? true : false; */\n\n  if (role === \"superAdmin\") return <AdminMenu />;\n\n  if (role === \"admin\") return <EmployeeMenu />;\n\n  return <UserMenu />;\n};\n","/Users/petrsavrasov/Projects/queueApp/client/src/http/index.js",[],"/Users/petrsavrasov/Project