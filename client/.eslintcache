;\n  } */\n  return (\n    <div className={styles.MainContainer}>\n      <div className={styles.Header}>\n        <div>\n          <SearchForm>\n            <Input\n              placeholder=\"Поиск посетителя по фамилии\"\n              id=\"filterUser\"\n              name=\"filterUser\"\n              type=\"text\"\n              value={visitor}\n              onChange={handleChange}\n              onKeyPress={pressHandler}\n            />\n          </SearchForm>\n        </div>\n\n        <div>\n          <Calendar props={{ updateDate, date }} />\n        </div>\n      </div>\n\n      <div>\n        <List props={{ userId, token, date, name }} />\n      </div>\n    </div>\n  );\n};\n","/Users/petrsavrasov/Projects/queueApp/client/src/components/Staff/StaffProfile.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/components/RegisterForm/RegisterForm.js",[],"/Users/petrsavrasov/Projects/queueApp/client/src/components/Tickets/newRecord/RecordForm.js",["388","389","390"],"import React, { useEffect, useState, useContext } from \"react\";\nimport { DropDown } from \"../../../UI/DropDown/DropDown\";\nimport { Calendar } from \"../../../UI/Calendar/Calendar\";\nimport { Employee } from \"../../../components/Employee/Employee\";\nimport M from \"materialize-css\";\nimport styles from \"./record.module.css\";\nimport { ClientForm } from \"../../../components/ClientForm/ClientForm\";\nimport { FormFooter } from \"../../../components/FormFooter/FormFooter\";\nimport { formatDate } from \"../../../utils/formatDate\";\nimport { useHttp } from \"../../../hooks/http.hook\";\nimport { TimeTable } from \"../../../components/TimeTable/TimeTable\";\nimport { RadioSelect } from \"../../../UI/RadioSelect/RadioSelect\";\nimport { AuthContext } from \"../../../context/AuthContext\";\n//import { useAuth } from \"../../../hooks/auth.hook\";\nimport { useMessage } from \"../../../hooks/message.hook\";\nimport { useSelector } from \"react-redux\";\nimport api from \"../../../http\";